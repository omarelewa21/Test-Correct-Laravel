{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": "^8.1",
		"aacotroneo/laravel-saml2": "^2.1",
		"artisaninweb/laravel-soap": "0.3.0.10",
		"barryvdh/laravel-dompdf": "^2.0",
		"barryvdh/laravel-snappy": "^1.0",
		"betterapp/laravel-db-encrypter": "^1.0",
		"bonecms/laravel-captcha": "^2.2",
		"bugsnag/bugsnag-laravel": "^2.0",
		"doctrine/dbal": "^2.9",
		"dyrynda/laravel-efficient-uuid": "^4.1",
		"dyrynda/laravel-model-uuid": "^6.2",
		"fideloper/proxy": "^4.0",
		"iio/libmergepdf": "^4.0",
		"laravel/framework": "^9.0",
		"laravel/legacy-factories": "^1.3",
		"laravel/telescope": "^4.0",
		"laravel/tinker": "^2.4",
		"league/glide-laravel": "^1.0",
		"livewire/livewire": "^2.4",
		"maatwebsite/excel": "^3.1",
		"mews/purifier": "^3.2",
		"pusher/pusher-php-server": "^5.0",
		"ramsey/uuid": "^4.1",
		"spatie/laravel-ignition": "^1.0",
		"symfony/http-client": "^6.1",
		"symfony/mailgun-mailer": "^6.1",
		"wire-elements/modal": "^1.0",
		"zanysoft/laravel-zip": "^1.0"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.5",
		"beyondcode/laravel-dump-server": "^1.0",
		"brianium/paratest": "^6.8",
		"filp/whoops": "~2.0",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "~1.0",
		"nunomaduro/collision": "^6.1",
		"phpunit/phpunit": "^9.0"
	},
	"autoload": {
		"exclude-from-classmap":[
			"Aacotroneo\\Saml2\\Saml2Auth"
		],
		"files": [
			"_overrides/Saml2Auth.php",
			"app/Http/helpers.php"
		],
		"psr-4": {
			"tcCore\\": "app/",
			"Database\\Seeders\\": "database/seeds/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"@php artisan clear-compiled"
		],
		"post-update-cmd": [
			"@php artisan clear-compiled"
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"minimum-stability": "stable"
}
