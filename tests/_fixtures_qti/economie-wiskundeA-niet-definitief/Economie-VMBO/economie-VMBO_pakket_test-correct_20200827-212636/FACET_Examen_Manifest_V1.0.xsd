					<xsd:documentation>fundamenteel niveau havo en mbo-4, vwo (rekenen)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4F">
				<xsd:annotation>
					<xsd:documentation>fundamenteel niveau vwo (taal)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1S">
				<xsd:annotation>
					<xsd:documentation>streefniveau basisonderwijs</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2S">
				<xsd:annotation>
					<xsd:documentation>streefniveau vmbo en mbo-1, mbo-2 en mbo-3</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3S">
				<xsd:annotation>
					<xsd:documentation>streefniveau havo en mbo-4, vwo (rekenen)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4S">
				<xsd:annotation>
					<xsd:documentation>streefniveau vwo (taal)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
