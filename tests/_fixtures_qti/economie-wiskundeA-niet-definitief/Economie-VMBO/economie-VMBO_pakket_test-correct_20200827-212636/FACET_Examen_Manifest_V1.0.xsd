		<xsd:restriction base="CodeType">
			<xsd:enumeration value="1F">
				<xsd:annotation>
					<xsd:documentation>fundamenteel niveau basisonderwijs</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="2F">
				<xsd:annotation>
					<xsd:documentation>fundamenteel niveau vmbo en mbo-1, mbo-2 en mbo-3</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3F">
